							id="frame2"
							margin_top="0.0"
							margin_bottom="0"
							margin_left="0"
							margin_right="0"
						>
							<box
								id="box5"
								orientation="horizontal"
								height="30"
								
							>
								<radio_button  id="radio_button1" label="Malade" />
								<radio_button  id="radio_button2" label="Suspect" />
