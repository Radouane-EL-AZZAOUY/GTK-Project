                            id="fixed1"
                        >
                            <combo_text_box 
                                id="combo1"
                                has_entry="true"
                                default_value="Choix de la marque"
                                is_editable="false"
                                popup_shown_rows="0"
                                option0="          HP             "
                                option1="          DELL"
                                option2="          ASSUS"
                                option3="          ACER"
                            />
                        </fixed>
                    </box>
                </box>
                <box 
                    id="checks_box"
                >
                    <label 
                        id="label3"
                        font_size="10"
                        label_text=" Son ergonomie"
                    />
                    <frame 
                        id="frame2"
                        label_yalign="0.500000"
