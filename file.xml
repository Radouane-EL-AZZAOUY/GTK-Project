                        />
                    </menu>
                </menu_item>
                <menu_item 
                    id="menu_item10"
                    label="LeurChoix"
                    use_underline="true"
                />
            </menu_bar>
            <box 
                id="content_box"
                margin_top="40"
                margin_left="20"
                spacing="20"
                halign="start"
                valign="start"
            >
                <box 
                    id="box3"
                    orientation="horizontal"
                    spacing="20"
                >
                    <box 
                        id="box4"
                    >
                        <label 
                            id="label1"
                            font_size="10"
                            label_text="La qualite de votre ordinateur"
                        />
                        <frame 
                            id="frame1"
                            label_yalign="0.500000"
                        >
                            <box 
                                id="box5"
                                orientation="horizontal"
                                spacing="20"
                            >
                                <radio_button 
                                    id="rbtn1"
                                    label="Très bonne"
                                    is_selected="true"
                                    is_button_mode="true"
                                />
                                <radio_button 
                                    id="rbtn2"
                                    label="Moyen"
                                    is_button_mode="true"
                                />
                                <radio_button 
