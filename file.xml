                                halign="start"
                                valign="start"
                            >
                                <radio_button 
                                    id="rbtn4"
                                    label="Très bonne"
                                    is_selected="true"
                                    is_button_mode="true"
                                />
                                <radio_button 
                                    id="rbtn5"
                                    label="Moyen"
                                    is_button_mode="true"
                                />
                                <radio_button 
                                    id="rbtn6"
                                    label="faible     "
                                    is_button_mode="true"
                                />
                            </box>
                        </frame>
                    </box>
