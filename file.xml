                />
                <menu_item 
                    id="menu_item2"
                    label="_Portable"
                    use_underline="true"
                >
                    <menu 
                        id="menu1"
                        bg_color="#FFFFFF"
                        monitor_num="-1"
                    >
                        <menu_item 
                            id="menu_item4"
                            label="_Marque"
                            use_underline="true"
                        />
                        <menu_item 
                            id="menu_item5"
                            label="Puissance"
                            use_underline="true"
