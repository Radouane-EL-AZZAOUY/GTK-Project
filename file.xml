            <fixed 
                id="fixed2"
            >
                <box 
                    id="box11"
                    position_x="450"
                    position_y="140"
                    orientation="horizontal"
                    spacing="20"
                    halign="start"
                    valign="start"
                >
                    <button 
                        id="btn_ok"
                        on_click="sig_dialog"
                        label="_Ok"
                        is_visible="false"
                        use_underline="true"
                    />
                    <button 
                        id="btn_quit"
                        on_click="sig_destroy"
                        label="_Quiter"
                        is_visible="false"
                        use_underline="true"
                    />
                </box>
            </fixed>
        </box>
    </scrolled_window>
</dialog>
