                            >
                                <menu_item 
                                    id="menu_item6"
                                    menu_right="1"
                                    label="Processor"
                                    use_underline="true"
                                />
                                <menu_item 
                                    id="menu_item7"
                                    menu_left="1"
                                    menu_right="2"
                                    label="Viresse"
                                    use_underline="true"
                                />
                                <menu_item 
                                    id="menu_item8"
                                    menu_left="2"
                                    menu_right="3"
                                    label="Disque dur"
                                    use_underline="true"
                                />
                            </menu>
                        </menu_item>
                        <menu_item 
                            id="menu_item9"
                            label=" _Prix"
